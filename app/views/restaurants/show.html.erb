<h1>
  <%= @restaurant.name %>
</h1>

<h4>Phone Number</h4>
<p>
  <%= @restaurant.phone_number %>
</p>
<h4>Address</h4>
<p>
  <%= @restaurant.address %>
</p>
<h4>Category</h4>
<p>
  <%= @restaurant.category %>
</p>
<h4>Reviews</h4>
<ul>
  <% @restaurant.reviews.each do |review| %>
    <li><%= review.content %></li>
  <%end  %>
</ul>

<%= link_to 'Add a review',new_restaurant_review_path(@restaurant)%>
<%= link_to 'Go to home',restaurants_path%>
